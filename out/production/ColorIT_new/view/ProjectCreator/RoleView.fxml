<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="280.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProjectCreator.RoleViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="63.0" prefWidth="394.0" spacing="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <Label fx:id="nameOfPerson" text="Name">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets left="15.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutX="2.0" layoutY="181.0" prefHeight="63.0" prefWidth="394.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#backOnClick" prefWidth="100.0" text="Back">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="63.0" prefHeight="113.0" prefWidth="396.0" spacing="50.0">
         <padding>
            <Insets left="15.0" />
         </padding>
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="150.0" spacing="20.0">
               <children>
                  <Button fx:id="scrum" mnemonicParsing="false" onAction="#scrumOnClick" prefWidth="140.0" text="Scrum Master">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="projectCreator" mnemonicParsing="false" onAction="#projectCreatorOnClick" prefWidth="140.0" text="Project Creator">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="113.0" prefWidth="150.0" spacing="20.0">
               <children>
                  <Button fx:id="projectOwner" mnemonicParsing="false" onAction="#projectOwnerOnClick" prefWidth="140.0" text="Project Owner">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="developer" mnemonicParsing="false" onAction="#developerOnClick" prefWidth="140.0" text="Developer">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
