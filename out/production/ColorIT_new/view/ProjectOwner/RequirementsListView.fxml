<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="610.0" prefWidth="1013.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ProjectOwner.RequirementsListViewController">
   <children>
      <TableView fx:id="RequirementsList" layoutY="100.0" prefHeight="320.0" prefWidth="990.0">
        <columns>
          <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="title" prefWidth="75.0" text="title" />
            <TableColumn fx:id="what" prefWidth="75.0" text="what" />
            <TableColumn fx:id="who" prefWidth="75.0" text="who" />
            <TableColumn fx:id="why" prefWidth="75.0" text="why" />
            <TableColumn fx:id="startedDate" prefWidth="108.0" text="started date" />
            <TableColumn fx:id="deadline" prefWidth="75.0" text="deadline" />
            <TableColumn fx:id="spendetTime" prefWidth="108.0" text="spendet time" />
            <TableColumn fx:id="estimatedTime1" prefWidth="108.0" text="estimeted time" />
            <TableColumn fx:id="responsiblePerson" prefWidth="137.0" text="responsible person" />
            <TableColumn fx:id="status" prefWidth="75.0" text="status" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" layoutY="492.0" prefHeight="75.0" prefWidth="990.0" spacing="50.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#backOnClick" prefWidth="120.0" text="Back">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="add" mnemonicParsing="false" onAction="#addOnClick" prefWidth="120.0" text="Add">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="edit" layoutX="270.0" layoutY="21.0" mnemonicParsing="false" onAction="#editOnClick" prefWidth="120.0" text="Edit">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="assignPerson" mnemonicParsing="false" onAction="#assignePersonOnClick" prefWidth="250.0" text="Assign Person">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="delete1" layoutX="765.0" layoutY="21.0" mnemonicParsing="false" onAction="#deleteOnClick" prefWidth="120.0" text="Delete">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="990.0" spacing="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Project:">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <Label fx:id="titleLabel" text="title of project">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="420.0" prefHeight="75.0" prefWidth="990.0" spacing="150.0">
         <children>
            <Button fx:id="disaprove" mnemonicParsing="false" onAction="#disaproveOnClick" prefWidth="250.0" text="DIsaprove">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="approve" layoutX="765.0" layoutY="21.0" mnemonicParsing="false" onAction="#approveOnClick" prefWidth="250.0" text="Approve">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
