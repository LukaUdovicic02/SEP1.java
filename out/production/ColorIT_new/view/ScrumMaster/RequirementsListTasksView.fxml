<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="430.0" prefWidth="563.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ScrumMaster.RequirementsListTasksViewController">
   <children>
      <TableView prefHeight="320.0" prefWidth="558.0">
         <columns>
            <TableColumn fx:id="id" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="title" prefWidth="75.0" text="title" />
            <TableColumn fx:id="what" prefWidth="75.0" text="what" />
            <TableColumn fx:id="who" prefWidth="75.0" text="who" />
            <TableColumn fx:id="why" prefWidth="75.0" text="why" />
            <TableColumn fx:id="deadline" prefWidth="75.0" text="deadline" />
            <TableColumn fx:id="estimatedTime" prefWidth="108.0" text="estimeted time" />
         </columns>
      </TableView>
      <HBox alignment="CENTER" layoutY="320.0" prefHeight="75.0" prefWidth="558.0" spacing="100.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#backOnClick" prefWidth="120.0" text="Back">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="show" mnemonicParsing="false" onAction="#showOnClick" prefWidth="200.0" text="Show Tasks">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
