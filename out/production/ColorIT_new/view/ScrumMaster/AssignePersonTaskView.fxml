<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="476.0" prefWidth="478.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ScrumMaster.AssignePersonTaskViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="54.0" prefWidth="464.0" spacing="20.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name:">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <Label fx:id="nameLabel" text="Label">
               <font>
                  <Font size="25.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" layoutY="351.0" prefHeight="100.0" prefWidth="464.0" spacing="80.0">
         <children>
            <Button fx:id="back" mnemonicParsing="false" onAction="#backOnClick" prefWidth="120.0" text="Back">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
            <Button fx:id="assigne" mnemonicParsing="false" onAction="#assigneOnClick" prefWidth="120.0" text="Assigne">
               <font>
                  <Font size="25.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <TableView fx:id="table" layoutY="66.0" prefHeight="274.0" prefWidth="464.0">
         <columns>
            <TableColumn fx:id="name" prefWidth="124.0" text="name" />
            <TableColumn fx:id="role" prefWidth="164.0" text="role" />
            <TableColumn fx:id="email" prefWidth="175.0" text="email" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
